        <!-- Tabbed subwindows for different search screens -->
        <TabBox item = "CTW_Pages">
                <ScreenID>CTW_Pages</ScreenID>
                <RelativePosition>true</RelativePosition>

                <AutoStretch>true</AutoStretch>
                
                <TopAnchorToTop>true</TopAnchorToTop>
                <BottomAnchorToTop>false</BottomAnchorToTop>
                <LeftAnchorToLeft>true</LeftAnchorToLeft>
                <RightAnchorToLeft>false</RightAnchorToLeft>
                
                <TopAnchorOffset>0</TopAnchorOffset>
                <BottomAnchorOffset>20</BottomAnchorOffset>
                <LeftAnchorOffset>0</LeftAnchorOffset>
                <RightAnchorOffset>0</RightAnchorOffset>

                <TabBorderTemplate>FT_DefTabBorder</TabBorderTemplate>
                <PageBorderTemplate>FT_DefPageBorder</PageBorderTemplate>
                        <Pages>CTW_TimerListPage</Pages>
                        <Pages>CTW_SettingsPage</Pages>
        </TabBox>

        <!-- Page for getting LFP players -->
        <Page item = "CTW_TimerListPage">
                <ScreenID>CTW_TimerListPage</ScreenID>
                <RelativePosition>true</RelativePosition>

                <!-- Pages are sized and positioned by their parent tab box -->

                <Style_VScroll>false</Style_VScroll>
                <Style_HScroll>false</Style_HScroll>
                <Style_Transparent>false</Style_Transparent>
                <DrawTemplate>WDT_Def</DrawTemplate>
                <TabText>Timers</TabText>
                <TabTextColor>
                                <R>255</R>
                                <G>255</G>
                                <B>255</B>
                </TabTextColor>
                <TabTextActiveColor>
                                <R>255</R>
                                <G>255</G>
                                <B>0</B>
                </TabTextActiveColor>
                <Style_Sizable>true</Style_Sizable>
                        <Pieces>CTW_TimerList</Pieces>
        </Page>
        
